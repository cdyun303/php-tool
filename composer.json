{
  "name": "cdyun/php-tool",
  "description": "PHP8.1+通用工具包，包含数组处理、字符串处理、时间处理、加解密、IP、地理计算、Curl、全局辅助方法等功能",
  "license": "MIT",
  "keywords": [
    "php",
    "cdyun",
    "array",
    "string",
    "time",
    "message",
    "crypto",
    "tree",
    "ip",
    "geo",
    "helper",
    "curl",
    "tools"
  ],
  "authors": [
    {
      "name": "cdyun",
      "email": "121625706@qq.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-openssl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-gd": "*",
    "ext-sodium": "*",
    "guzzlehttp/guzzle": "^7.9"
  },
  "autoload": {
    "psr-4": {
      "Cdyun\\PhpTool\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Cdyun\\PhpTool\\Tests\\": "tests/"
    }
  }
}