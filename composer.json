{
  "name": "cdyun/php-tool",
  "description": "PHP8.1+通用工具包，包含数组处理、字符串处理、时间处理、加解密、IP、地理计算、Curl、二维码生成、全局辅助方法等功能",
  "license": "MIT",
  "keywords": [
    "php",
    "cdyun",
    "array",
    "string",
    "time",
    "message",
    "crypto",
    "tree",
    "ip",
    "geo",
    "helper",
    "curl",
    "qrcode",
    "tools"
  ],
  "authors": [
    {
      "name": "cdyun",
      "email": "121625706@qq.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-openssl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-gd": "*",
    "endroid/qr-code": "^6.0",
    "ext-sodium": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.20"
  },
  "autoload": {
    "psr-4": {
      "Cdyun\\PhpTool\\": "src/"
    },
    "files": [
      "src/Helper/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Cdyun\\PhpTool\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "phpstan analyse",
    "fix": "php-cs-fixer fix"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}